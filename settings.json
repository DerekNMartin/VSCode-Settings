    "terminal.integrated.shell.windows": "C:\\Windows\\System32\\bash.exe",
    "workbench.iconTheme": "easy-icons",
    "window.zoomLevel": 0,
    "editor.fontFamily": "Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
}